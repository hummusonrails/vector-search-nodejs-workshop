{
    "name": "Couchbase Node.js Workspace",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "couchbase.vscode-couchbase"
            ]
        }
    },
    "postCreateCommand": [
        ". \"$HOME/.cargo/env\"",
        "npm install"
    ],
    "features": {
        "ghcr.io/devcontainers/features/node:1": {}
    }
}
